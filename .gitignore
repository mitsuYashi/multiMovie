# See https://help.github.com/articles/ignoring-files/ for more about ignoring files.

# dependencies
app/node_modules
app/.pnp
app/.pnp.js

# testing
app/coverage

# next.js
app/.next
app/out/

# production
app/build

# misc
app/.DS_Store
app/*.pem

# debug
app/npm-debug.log*
app/yarn-debug.log*
app/yarn-error.log*
app/.pnpm-debug.log*

# local env files
app/.env.local
app/.env.development.local
app/.env.test.local
app/.env.production.local

# vercel
app/.vercel

# typescript
app/*.tsbuildinfo

# Ignore db data
/db

# Ignore bundler config.
api/.bundle

# Ignore all logfiles and tempfiles.
api/log/
api/tmp/
api/!/log/.keep
api/!/tmp/.keep

# Ignore pidfiles, but keep the directory.
/tmp/pids/*
!/tmp/pids/
!/tmp/pids/.keep

# Ignore uploaded files in development.
api/storage/
api/!/storage/.keep
api/.byebug_history

# Ignore master key for decrypting credentials and more.
api/config/master.key
